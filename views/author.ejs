            <div class="left_1-2"><%=user.name %></div>
            <div class="left_1-3"><%=user.profession %></div>
            <div class="left-icons">
              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icon/Facebook.svg" alt="" class="icon-1" />
                </div>
              </a>

              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icon/instagram.svg" alt="" class="icon-1" />
                </div>
              </a>

              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icon/Twitter.svg" alt="" class="icon-1" />
                </div>
              </a>

              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icon/Linkeding.svg" alt="" class="icon-1" />
                </div>
              </a>

              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icon/Youtube.svg" alt="" class="icon-1" />
