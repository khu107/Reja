        <div class="right_frame">
          <div class="right_frame_dark">
            <img src="/author/icon/dark.svg" alt="" />
          </div>
          <div class="right_frame_icon">
            <div class="right_frame_icon_home">
              <img src="/author/icon/home.svg" alt="" />
            </div>
            <div class="right_frame_icons">
              <img src="/author/icon/right_home.svg" alt="" />
              <img src="/author/icon/Service.svg" alt="" />
              <img src="/author/icon/cv.svg" alt="" />
              <img src="/author/icon/Portfolio.svg" alt="" />
              <img src="/author/icon/Blog.svg" alt="" />
              <img src="/author/icon/Contact.svg" alt="" />
            </div>
          </div>
